{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "operator-linebreak":"off",
        "comma-dangle":"off",
        "import/extensions":"off",
        "import/prefer-default-export":"off",
        "object-curly-newline":"off",
        "camelcase":"off",
        "prefer-destructuring": "off",
        "indent": "off",
        "max-len": "off",
        "no-plusplus": "off",
        "no-await-in-loop": "off"
    },
    "overrides": [
        {
            "files": [
              "**/*.test.js"
            ],
            "env": {
              "jest": true
            }
          }
    ]
}
