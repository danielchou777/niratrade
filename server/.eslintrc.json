{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "operator-linebreak":"off",
        "comma-dangle":"off",
        "import/extensions":"off",
        "import/prefer-default-export":"off",
        "object-curly-newline":"off",
        "camelcase":"off",
        "prefer-destructuring": "off",
        "indent": "off",
        // "no-param-reassign":"off",
        // "no-unused-vars": "off",
        "no-await-in-loop": "off"
        // "prefer-const": "off",
        // "no-shadow": "off"
    }
}
